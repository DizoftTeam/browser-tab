{
  "name": "browser-tab",
  "displayName": "browser-tab",
  "description": "Open Broser Tab in VSCode",
  "version": "0.0.1",
  "publisher": "DIZOFT",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.browsertab"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.browsertab",
        "title": "Open Browser Tab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "axios": "0.18.0"
  }
}
