{
  "name": "browser-tab",
  "displayName": "browser-tab",
  "description": "Open Broser Tab in VSCode",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DizoftTeam/browser-tab.git"
  },
  "homepage": "https://github.com/DizoftTeam/browser-tab/blob/master/README.md",
  "publisher": "DIZOFT",
  "engines": {
    "vscode": "^1.25.0"
  },
  "author": {
    "email": "ruslan.mahin1@gmail.com",
    "name": "Ruslan Makhin (WiRight)"
  },
  "maintainers": [
    {
      "email": "ruslan.mahin1@gmail.com",
      "name": "Ruslan Makhin (WiRight)"
    }
  ],
  "galleryBanner": {
    "color": "#29b768",
    "theme": "light"
  },
  "keywords": [
    "chrome",
    "browser",
    "tab",
    "dizoft"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.browsertab"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.browsertab",
        "title": "Open Browser Tab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "axios": "0.18.0"
  }
